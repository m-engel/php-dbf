{
    "name": "totalcrm/php-dbf",
    "type": "library",
    "description": "A simple parser for *.dbf files using PHP",
    "keywords": ["dbase", "dbf", "php"],
    "homepage": "https://github.com/totalcrm/php-dbf",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Romanenko",
            "email": "romanenko.a@bk.ru"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-iconv": "*"
    },
    "autoload": {
        "psr-4": {
            "TotalCRM\\DBase\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0||^8.0||^9.0"
    },
    "autoload-dev": {
        "psr-4": {
            "TotalCRM\\DBase\\Tests\\": "tests"
        }
    },
    "version": "dev-master"
}
